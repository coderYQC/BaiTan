# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'BaiTan' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BaiTan 
 
   source 'https://github.com/CocoaPods/Specs.git'
   source 'https://github.com/aliyun/aliyun-specs.git'
   pod 'AlicloudPush', '~> 1.9.8'
   pod 'Kingfisher'#图片加载库SDWebImage
   
   pod 'Alamofire'#类似AFNetworking的网络基础库
   pod 'AlamofireObjectMapper'#使用ObjectMapper可以让JSON数据直接转化为对象
   pod 'DaisyNet'#网络请求缓存框架
   pod 'Cache'#Cache是配合DaisyNet一起使用的
  
   pod 'SwiftyJSON'#json数据解析的œ
   pod 'ReachabilitySwift'#网络状态监控
   pod 'IQKeyboardManager'#键盘处理
   pod 'BaiduMapKit','~>4.1.1'#百度地图
   pod 'IQKeyboardManager'
   pod 'OpenShare'#超轻量级分享框架
   pod 'SVProgressHUD'#简单易用的HUD,类似MBProgressHUD
   pod 'FDFullscreenPopGesture'#手势左滑返回
   
   pod 'Masonry'
   pod 'SnapKit'#masonry
   pod 'ZLPhotoBrowser'#拍照和视频录制框架
   pod 'mob_sharesdk'#分享
   pod 'mob_sharesdk','~> 4.2.4'#分享
   pod 'mob_sharesdk/ShareSDKUI'
   pod 'mob_sharesdk/ShareSDKExtension'
   pod 'mob_sharesdk/ShareSDKPlatforms/QQ'
   pod 'mob_sharesdk/ShareSDKPlatforms/WeChat'
   
   pod 'ESTabBarController-swift','~> 2.6.0'#自定义TabBarController

end
